{
  "projects": {
    "default": <default-dev-project>,
    "prod": <default-prod-project>,
    "dev": <default-dev-project>
  },
  "targets": {
    <default-dev-project>: {
      "hosting": {
        "next": [
          <next-dev-sub-project>
        ],
        "gatsby": [
          <gatsby-dev-sub-project>
        ]
      }
    },
    <default-prod-project>: {
      "hosting": {
        "next": [
          <next-prod-sub-project>
        ],
        "gatsby": [
          <gatsby-prod-sub-project>
        ]
      }
    }
  }
}